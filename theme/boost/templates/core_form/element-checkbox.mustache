<div class="form-group row {{#error}}has-danger{{/error}} fitem {{#advanced}}advanced{{/advanced}} {{{element.extraclasses}}}">
    <div class="col-md-9 push-md-3 checkbox">
        <label>
        {{^element.hardfrozen}}
            {{#element.frozen}}
                <input type="hidden" name="{{element.name}}" value="{{element.frozenvalue}}">
            {{/element.frozen}}
        {{/element.hardfrozen}}
        <input type="checkbox"
            name="{{element.name}}"
            {{#element.value}}
                value="{{element.value}}"
            {{/element.value}}
            {{^element.value}}
                value="1"
            {{/element.value}}
            id="{{element.id}}" {{#element.checked}}checked{{/element.checked}}
            {{#error}}
                autofocus aria-describedby="id_error_{{element.name}}"
            {{/error}}
            {{#element.frozen}}
                disabled
            {{/element.frozen}}
            {{{element.attributes}}} >
            {{{label}}}
        </label> {{{helpbutton}}}
        <div class="form-control-feedback" id="id_error_{{element.name}}" {{^error}} style="display: none;"{{/error}}>
            {{{error}}}
        </div>
    </div>
</div>
{{^element.frozen}}
{{#js}}
require(['theme_boost/form-display-errors'], function(module) {
    module.enhance({{#quote}}{{element.id}}{{/quote}});
});
{{/js}}
{{/element.frozen}}
