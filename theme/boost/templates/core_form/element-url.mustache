{{< core_form/element-template }}
    {{$element}}
        {{^element.frozen}}
        <input type="url"
                class="form-control {{#error}}form-control-danger{{/error}}"
                name="{{element.name}}"
                id="{{element.id}}"
                value="{{element.value}}"
                size="{{element.size}}"
                {{#error}}
                    autofocus aria-describedby="id_error_{{element.name}}"
                {{/error}}
                {{{element.attributes}}} >
                {{{element.filepickerhtml}}}
        {{/element.frozen}}
        {{#element.frozen}}
            {{element.value}}
        {{/element.frozen}}
    {{/element}}
{{/ core_form/element-template }}
